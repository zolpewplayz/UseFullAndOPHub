b:Button("Spinning Hat")
   local plr = game.Players.LocalPlayer;
local chr = plr.Character;
local hum = chr.Humanoid;
local mov = {};
local mov2 = {};

--sub to me_ozoneYT

for i,v in next, game:GetService("Players").LocalPlayer.Character:GetDescendants() do
